    "from scipy.stats import gennorm\n",
    "import matplotlib.pyplot as plt\n",
    "import numpy as np\n",
    "fig, ax = plt.subplots(1, 1)\n",
    "\n",
    "\n",
    "#generalized norm\n",
    "beta = 1.3\n",
    "x = np.linspace(gennorm.ppf(0.01, 1.3),\n",
    "                gennorm.ppf(0.99, 1.3), 100)\n",
    "ax.plot(x, gennorm.pdf(x, beta))"
   ]
